{
  "short_name": "Trackr",
  "name": "Trackr",
  "version": "1.0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "author": "Keegan Borig",
  "permissions": [
    "activeTab"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
